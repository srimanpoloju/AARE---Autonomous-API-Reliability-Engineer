# PostgreSQL Configuration
POSTGRES_USER=aare_user
POSTGRES_PASSWORD=aare_password
POSTGRES_DB=aare_db

# RabbitMQ Configuration
RABBITMQ_USER=guest
RABBITMQ_PASS=guest

# Grafana Admin Credentials
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# OpenAI API Key (optional)
# If not provided, AI-driven RCA reports will be skipped.
OPENAI_API_KEY=your-openai-api-key

# AARE UI Authentication
# Default admin user credentials for the UI
AARE_ADMIN_USER=admin
AARE_ADMIN_PASSWORD=password
# Secret for signing JWTs. Must be a long, random string.
AARE_JWT_SECRET=aare-super-secret-key-for-jwt-signing-32-chars